{
	"compilerOptions": {
		"rootDir": ".",
		"outDir": "dist",
		"target": "es2022",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	}
}
